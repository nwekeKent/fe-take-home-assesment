{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "types/**/*.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "node",
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "outDir": "dist/types",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  }
}